{
  "name": "college-backend",
  "version": "1.0.0",
  "description": "Backend для системы колледжа",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node scripts/generate-secrets.js",
    "security-check": "node scripts/security-check.js",
    "secure-db": "node scripts/secure-database.js",
    "secure-db:watch": "node scripts/secure-database.js --watch",
    "secure-db:watch5": "node scripts/secure-database.js --watch --interval=5",
    "audit": "npm audit && node scripts/security-check.js",
    "prestart": "node scripts/security-check.js",
    "prod": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "college",
    "education",
    "api"
  ],
  "author": "College System",
  "license": "MIT"
}
